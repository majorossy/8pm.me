{
    "name": "archivedotorg/module-core",
    "description": "Enterprise-grade module for importing live music recordings from Archive.org into Magento 2",
    "type": "magento2-module",
    "license": "proprietary",
    "version": "2.0.0",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
        "magento/framework": "^103.0",
        "magento/module-catalog": "^104.0",
        "magento/module-eav": "^102.0",
        "magento/module-store": "^101.0",
        "magento/module-checkout": "^100.0",
        "magento/module-config": "^101.0",
        "magento/module-cron": "^100.0"
    },
    "suggest": {
        "magento/module-catalog-url-rewrite": "For URL rewrite functionality"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "ArchiveDotOrg\\Core\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "repo.magento.com": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "ArchiveDotOrg/Core"
            ]
        ]
    },
    "keywords": [
        "magento2",
        "archive.org",
        "music",
        "import",
        "live recordings"
    ]
}
