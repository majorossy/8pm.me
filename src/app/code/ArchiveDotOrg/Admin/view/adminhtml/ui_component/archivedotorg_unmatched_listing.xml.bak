<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">archivedotorg_unmatched_listing.archivedotorg_unmatched_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <buttons>
            <button name="export_yaml">
                <url path="*/*/exportYaml"/>
                <class>primary</class>
                <label translate="true">Export to YAML Template</label>
            </button>
        </buttons>
        <spinner>archivedotorg_unmatched_columns</spinner>
        <deps>
            <dep>archivedotorg_unmatched_listing.archivedotorg_unmatched_listing_data_source</dep>
        </deps>
    </settings>
    <dataSource name="archivedotorg_unmatched_listing_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <storageConfig>
                <param name="indexField" xsi:type="string">unmatched_id</param>
            </storageConfig>
            <updateUrl path="mui/index/render"/>
        </settings>
        <aclResource>ArchiveDotOrg_Admin::unmatched</aclResource>
        <dataProvider class="ArchiveDotOrg\Admin\Ui\Component\Listing\UnmatchedDataProvider" name="archivedotorg_unmatched_listing_data_source">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>unmatched_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <listingToolbar name="listing_top">
        <settings>
            <sticky>true</sticky>
        </settings>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters">
            <settings>
                <templates>
                    <filters>
                        <select>
                            <param name="template" xsi:type="string">ui/grid/filters/elements/ui-select</param>
                            <param name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</param>
                        </select>
                    </filters>
                </templates>
            </settings>
            <filterSelect name="artist_id" provider="${ $.parentName }">
                <settings>
                    <caption translate="true">Select Artist</caption>
                    <dataScope>artist_id</dataScope>
                    <label translate="true">Artist</label>
                </settings>
            </filterSelect>
            <filterSelect name="resolved" provider="${ $.parentName }">
                <settings>
                    <caption translate="true">Select Status</caption>
                    <dataScope>resolved</dataScope>
                    <label translate="true">Resolved</label>
                    <options class="Magento\Config\Model\Config\Source\Yesno"/>
                </settings>
            </filterSelect>
        </filters>
        <massaction name="listing_massaction">
            <action name="mark_resolved">
                <settings>
                    <confirm>
                        <message translate="true">Mark selected tracks as resolved?</message>
                        <title translate="true">Mark as Resolved</title>
                    </confirm>
                    <url path="archivedotorg/unmatched/massResolve"/>
                    <type>mark_resolved</type>
                    <label translate="true">Mark as Resolved</label>
                </settings>
            </action>
            <action name="mark_unresolved">
                <settings>
                    <confirm>
                        <message translate="true">Mark selected tracks as unresolved?</message>
                        <title translate="true">Mark as Unresolved</title>
                    </confirm>
                    <url path="archivedotorg/unmatched/massUnresolve"/>
                    <type>mark_unresolved</type>
                    <label translate="true">Mark as Unresolved</label>
                </settings>
            </action>
        </massaction>
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="archivedotorg_unmatched_columns">
        <settings>
            <editorConfig>
                <param name="enabled" xsi:type="boolean">false</param>
            </editorConfig>
        </settings>
        <selectionsColumn name="ids">
            <settings>
                <indexField>unmatched_id</indexField>
            </settings>
        </selectionsColumn>
        <column name="track_name">
            <settings>
                <filter>text</filter>
                <label translate="true">Track Name</label>
                <sortOrder>10</sortOrder>
            </settings>
        </column>
        <column name="artist_name">
            <settings>
                <filter>text</filter>
                <label translate="true">Artist</label>
                <sortOrder>20</sortOrder>
            </settings>
        </column>
        <column name="occurrence_count">
            <settings>
                <filter>textRange</filter>
                <label translate="true">Occurrences</label>
                <sortOrder>30</sortOrder>
            </settings>
        </column>
        <column name="suggested_match">
            <settings>
                <filter>text</filter>
                <label translate="true">Suggested Match</label>
                <sortOrder>40</sortOrder>
            </settings>
        </column>
        <column name="show_identifier">
            <settings>
                <filter>text</filter>
                <label translate="true">Example Show</label>
                <sortOrder>50</sortOrder>
            </settings>
        </column>
        <column name="first_seen_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">First Seen</label>
                <sortOrder>60</sortOrder>
            </settings>
        </column>
        <column name="last_seen_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">Last Seen</label>
                <sortOrder>70</sortOrder>
            </settings>
        </column>
        <column name="resolved" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <filter>select</filter>
                <options class="Magento\Config\Model\Config\Source\Yesno"/>
                <dataType>select</dataType>
                <label translate="true">Resolved</label>
                <sortOrder>80</sortOrder>
            </settings>
        </column>
        <actionsColumn name="actions" class="ArchiveDotOrg\Admin\Ui\Component\Listing\Column\UnmatchedActions">
            <settings>
                <indexField>unmatched_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
                <sortOrder>90</sortOrder>
            </settings>
        </actionsColumn>
    </columns>
</listing>
