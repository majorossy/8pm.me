<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">archivedotorg_history_listing.archivedotorg_history_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <spinner>archivedotorg_history_columns</spinner>
        <deps>
            <dep>archivedotorg_history_listing.archivedotorg_history_listing_data_source</dep>
        </deps>
    </settings>
    <dataSource name="archivedotorg_history_listing_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <storageConfig>
                <param name="indexField" xsi:type="string">run_id</param>
            </storageConfig>
            <updateUrl path="mui/index/render"/>
        </settings>
        <aclResource>ArchiveDotOrg_Admin::history</aclResource>
        <dataProvider class="ArchiveDotOrg\Admin\Ui\Component\Listing\HistoryDataProvider" name="archivedotorg_history_listing_data_source">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>run_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <listingToolbar name="listing_top">
        <settings>
            <sticky>true</sticky>
        </settings>
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters">
            <settings>
                <templates>
                    <filters>
                        <select>
                            <param name="template" xsi:type="string">ui/grid/filters/elements/ui-select</param>
                            <param name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</param>
                        </select>
                    </filters>
                </templates>
            </settings>
            <filterSelect name="artist_id" provider="${ $.parentName }">
                <settings>
                    <caption translate="true">Select Artist</caption>
                    <dataScope>artist_id</dataScope>
                    <label translate="true">Artist</label>
                </settings>
            </filterSelect>
            <filterSelect name="command" provider="${ $.parentName }">
                <settings>
                    <caption translate="true">Select Command</caption>
                    <dataScope>command</dataScope>
                    <label translate="true">Command</label>
                    <options class="ArchiveDotOrg\Admin\Model\Source\CommandTypes"/>
                </settings>
            </filterSelect>
            <filterSelect name="status" provider="${ $.parentName }">
                <settings>
                    <caption translate="true">Select Status</caption>
                    <dataScope>status</dataScope>
                    <label translate="true">Status</label>
                    <options class="ArchiveDotOrg\Admin\Model\Source\ImportStatus"/>
                </settings>
            </filterSelect>
        </filters>
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="archivedotorg_history_columns">
        <settings>
            <editorConfig>
                <param name="enabled" xsi:type="boolean">false</param>
            </editorConfig>
        </settings>
        <selectionsColumn name="ids">
            <settings>
                <indexField>run_id</indexField>
            </settings>
        </selectionsColumn>
        <column name="correlation_id">
            <settings>
                <filter>text</filter>
                <label translate="true">Correlation ID</label>
                <sortOrder>10</sortOrder>
            </settings>
        </column>
        <column name="artist_name">
            <settings>
                <filter>text</filter>
                <label translate="true">Artist</label>
                <sortOrder>20</sortOrder>
            </settings>
        </column>
        <column name="command">
            <settings>
                <filter>text</filter>
                <label translate="true">Command</label>
                <sortOrder>30</sortOrder>
            </settings>
        </column>
        <column name="status" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <filter>select</filter>
                <options class="ArchiveDotOrg\Admin\Model\Source\ImportStatus"/>
                <dataType>select</dataType>
                <label translate="true">Status</label>
                <sortOrder>40</sortOrder>
            </settings>
        </column>
        <column name="started_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">Started At</label>
                <sortOrder>50</sortOrder>
            </settings>
        </column>
        <column name="completed_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
            <settings>
                <filter>dateRange</filter>
                <dataType>date</dataType>
                <label translate="true">Completed At</label>
                <sortOrder>60</sortOrder>
            </settings>
        </column>
        <column name="duration">
            <settings>
                <label translate="true">Duration</label>
                <sortOrder>70</sortOrder>
            </settings>
        </column>
        <column name="shows_processed">
            <settings>
                <filter>textRange</filter>
                <label translate="true">Shows Processed</label>
                <sortOrder>80</sortOrder>
            </settings>
        </column>
        <column name="tracks_processed">
            <settings>
                <filter>textRange</filter>
                <label translate="true">Tracks Processed</label>
                <sortOrder>90</sortOrder>
            </settings>
        </column>
        <column name="error_message">
            <settings>
                <label translate="true">Error Message</label>
                <sortOrder>100</sortOrder>
            </settings>
        </column>
        <actionsColumn name="actions" class="ArchiveDotOrg\Admin\Ui\Component\Listing\Column\HistoryActions">
            <settings>
                <indexField>run_id</indexField>
                <resizeEnabled>false</resizeEnabled>
                <resizeDefaultWidth>107</resizeDefaultWidth>
                <sortOrder>110</sortOrder>
            </settings>
        </actionsColumn>
    </columns>
</listing>
